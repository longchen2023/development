$(document).ready(function(){
	  //修正ボタンの事件
	  $("li").click(function(){
		  
		  var xx = window.parent.document..getElementById("menu2");;
		  var zz = xx.document.getElementsByClassName("li");
		  alert(xx);
		  
		  $("li").each(function(index){
		        $(this).removeClass("active");
		      })   
		  $(this).addClass("active");
		
	  });
	  
	  
});