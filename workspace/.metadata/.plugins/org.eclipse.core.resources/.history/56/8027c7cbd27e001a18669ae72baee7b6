$(document).ready(function(){
	  //修正ボタンの事件
	  $("li").click(function(){
		  
		  $("li").each(function(index){
		        $(this).removeClass("active");
		      })   
		  $(this).addClass("active");
		  
		  $("#ifm").contents().find("#btnOk").click();
	  });
	  
	  
	  var xx = $("#menu2",parent.document).find("li").val();
	  alert(xx);
	  
});